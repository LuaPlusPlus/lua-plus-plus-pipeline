{
	"name": "lpp-pipeline",
	"displayName": "lpp-pipeline",
	"description": "Makes using lua++ script easy.",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"publisher": "nosharp",
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "lpp",
			"properties": {
				"lpp.transpiler": {
					"type": "string",
					"default": "luapp",
					"description": "the luapp-with-dependencies.jar path."
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"child_process": "^1.0.2"
	}
}
